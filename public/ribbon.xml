<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="ribbon.OnAddinLoad">
    <ribbon startFromScratch="false">
        <tabs>
            <tab id="wpsAddinTab" label="HND插件" getVisible="ribbon.OnGetTabVisible">
                <group id="mainGroup" label="">
                    <!-- 填入图片按钮 -->
                <button id="btnFillImage" label="填入图片" onAction="ribbon.OnAction" enabled="true" getImage="ribbon.GetImage" getVisible="ribbon.OnGetVisible" size="large"/>
                <!-- 添加批注按钮 -->
                <button id="btnAddComment" label="添加批注" onAction="ribbon.OnAction" enabled="true" getImage="ribbon.GetImage" getVisible="ribbon.OnGetVisible" size="large"/>
                <!-- 物料查询按钮 -->
                <button id="btnMaterialQuery" label="物料查询" onAction="ribbon.OnAction" enabled="true" getImage="ribbon.GetImage" getVisible="ribbon.OnGetVisible" size="large"/>
                <!-- 动态时间按钮 -->
                <button id="btnDynamicTime" label="动态时间" onAction="ribbon.OnAction" enabled="true" getImage="ribbon.GetImage" getVisible="ribbon.OnGetVisible" size="large"/>

                </group>
            </tab>
        </tabs>
    </ribbon>
</customUI>